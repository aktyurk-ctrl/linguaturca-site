{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  },
  "regions": ["fra1"],
  "github": {
    "enabled": true,
    "autoJobCancelation": true
  },
  "git": {
    "deploymentEnabled": true,
    "productionBranch": "main"
  }
}
